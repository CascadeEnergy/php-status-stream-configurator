{
  "name": "cascade-energy/php-status-stream-configurator",
  "description": "Execution environment metadata DI configurator for PHP status streams",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Berube",
      "email": "charles.berube@cascadeenergy.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "CascadeEnergy\\StatusStream\\Configurator\\": "src"
    }
